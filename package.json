  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc lib/index.ts --outDir dist/",
    "test": "mocha",
    "build-test": "npm run build && npm run test",
    "lint": "standard lib && standard --env mocha test",
    "lint-fix": "standard --fix lib && standard --fix --env mocha test",
    "sync-1": "resin sync 426183b --source dist/ --destination /usr/src/app",
    "sync-2": "resin sync 30bae16 --source dist/ --destination /usr/src/app",
    "build-sync-1": "npm run build && npm run sync-1",
    "build-sync-2": "npm run build && npm run sync-2"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "dbus-native": "^0.2.5",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.21",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.110",
